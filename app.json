{
  "expo": {
    "name": "Food Delivery",
    "slug": "food-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./node_modules/@expo/snack-static/react-native-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./node_modules/@expo/snack-static/react-native-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a082"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00a082"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
